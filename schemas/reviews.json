{
    "$schema": "https://json-schema.org",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "user_id",
        "stars",
        "review"
    ],
    "properties": {
        "user_id": {
            "type": "integer",
            "default": 0,
            "title": "The user_id Schema",
            "examples": [
                2
            ]
        },
        "stars": {
            "type": "integer",
            "maximum": 5,
            "default": 0,
            "title": "The stars Schema",
            "examples": [
                5
            ]
        },
        "review": {
            "type": "string",
            "minLength": 1,
            "default": "",
            "title": "The reviews Schema",
            "examples": [
                "Good!"
            ]
        }
    },
    "examples": [{
        "user_id": 2,
        "stars": 5,
        "review": "Good!"
    }]
}